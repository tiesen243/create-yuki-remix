    "format": "prettier --check . --ignore-path .gitignore",
    "format:fix": "prisma format && prettier --write . --ignore-path .gitignore",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate"
  },
  "dependencies": {
